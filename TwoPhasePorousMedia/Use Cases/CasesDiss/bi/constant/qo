FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      qo;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField uniform 0;

boundaryField {
    left
    {
        type            fixedValue;
	value           uniform  0;
    }
    right
    {
        type            fixedValue;
	value           uniform  0;
    }
    up
    {
        type            fixedValue;
	value           uniform  0;
    }
    down
    {
        type            fixedValue;
	value           uniform  0;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
};




