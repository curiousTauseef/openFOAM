FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      kr_o;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField {
    left
    {
        type            fixedValue;
	value           uniform  1;
    }
    right
    {
        type            fixedValue;
	value           uniform  1;
    }
    up
    {
        type            empty;
    }
    down
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
};

