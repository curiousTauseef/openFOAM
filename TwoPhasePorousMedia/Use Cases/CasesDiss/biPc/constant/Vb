FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "constant";
    object      Vb;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 0 0 0 0 0];

internalField   uniform 1.25e+08;

boundaryField
{
    left
    {
        type            calculated;
        value           uniform 1.25e+08;
    }
    right
    {
        type            calculated;
        value           uniform 1.25e+08;
    }
    up
    {
        type            calculated;
        value           uniform 1.25e+08;
    }
    down
    {
        type            calculated;
        value           uniform 1.25e+08;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


