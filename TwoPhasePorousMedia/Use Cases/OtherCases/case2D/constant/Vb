FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Vb;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    left
    {
        type            calculated;
	value           uniform 1.0;
    }
    right
    {
        type            calculated;
	value           uniform 1.0;
    }
    up
    {
        type            calculated;
	value           uniform 1.0;
    }
    down
    {
        type            calculated;
	value           uniform 1.0;
    }
    frontBack
    {
        type            empty;
    }
}


