// Unit conversion facors: 
//(Customary Unit):
ALPHA_C         ALPHA_C [0 0 0 0 0 0 0] 5.614583;//1;
BETA_C          BETA_C  [0 0 0 0 0 0 0] 1.127;//86.4e-6;
GAMA_C          GAMA_C  [0 0 0 0 0 0 0] 0.21584e-3;//1e-3;
// Fluid properties:
mu_ref          mu_ref  [1 -1 -1 0 0 0 0] 10;
B_ref           B_ref   [0 0 0 0 0 0 0] 1;
c_f             c_f     [-1 1 2 0 0 0 0] 1e-5;
rho_sc          rho_sc  [1 -3 0 0 0 0 0] 50;
rho_ref         rho_ref [1 -3 0 0 0 0 0] 50;
// Rock properties:
c_r             c_r     [-1 1 2 0 0 0 0] 0;
// Reference Pressure:
Pref            Pref    [1 -1 -2 0 0 0 0] 400;
// Injector Well:
q_in            q_in    [0 0 -1 0 0 0 0] 10;  // qsc / Vb (m3/s)/m3 idem no q field
// Productor Well:
P_out           P_out   [1 -1 -2 0 0 0 0] 10;
// Local gravity:
G               G       [0 1 -2 0 0 0 0] 9.81;

