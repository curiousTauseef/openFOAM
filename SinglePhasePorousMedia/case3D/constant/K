FoamFile
{
    version     2.0;
    format      ascii;
    class       volTensorField;
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform (0.0887 0 0 0 0.0887 0 0 0 0.0887); // 100 mD

boundaryField {
    left
    {
        type            fixedValue;
        value           uniform (0.0887 0 0 0 0.0887 0 0 0 0.0887);
    }
    right
    {
        type            fixedValue;
        value           uniform (0.0887 0 0 0 0.0887 0 0 0 0.0887);
    }
    up
    {
        type            fixedValue;
        value           uniform (0.0887 0 0 0 0.0887 0 0 0 0.0887);
    }
    down
    {
        type            fixedValue;
        value           uniform (0.0887 0 0 0 0.0887 0 0 0 0.0887);
    }
    front
    {
        type            fixedValue;
        value           uniform (0.0887 0 0 0 0.0887 0 0 0 0.0887);
    }
    back
    {
        type            fixedValue;
        value           uniform (0.0887 0 0 0 0.0887 0 0 0 0.0887);
    }
};


